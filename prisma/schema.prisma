datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model userInfo {
  id       Int    @id @default(autoincrement())
  email    String @unique
  username String
  password String
  mode     String @default("normal") // Set default value to "normal"
}

model docInfo {
  doc_id   Int    @id @default(autoincrement())
  title    String
  content  String
  doc_link String
}